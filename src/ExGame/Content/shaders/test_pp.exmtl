    "glsl":
"
#version 410 core
layout (location = 0) out vec4 FragColor;
in vec2 UVcoords;

uniform sampler2D screen;

uniform sampler2D texTest;

void main() {
    FragColor = texture(screen, UVcoords) + texture(texTest, UVcoords) / 4;
    //FragColor = vec4(1.0, 0.8, 0.0, 0.0);
}
"
